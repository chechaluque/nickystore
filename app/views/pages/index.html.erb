    <div class="row">
      <div class="col-sm-4">
        <%= link_to root_path do %>
          <span data-icon="v" class="icon"></span> <span class="text">Entrega Inmediad</span>
        <% end %>
      </div>
      <div class="col-sm-4">
        <%= link_to  root_path do %>
          <span data-icon="/" class="icon"></span> <span class="text">Variedad y Estilos</span>
        <% end %>
