<header class="fullscreen-element">
  <div class="background-image-holder parallax-background overlay" data-stellar-background-ratio="0.9">
    <%= image_tag('nicky3.jpg', class: "background-image", alt: 'Background') %>

  </div>

  <div class="container align-vertical hero">
    <div class="row">
      <div class="col-md-5 col-sm-5 col-md-offset-7 col-sm-offset-7 text-left">
        <h1 style="color:#3f51b5;" >Productos de Calidad</h1>
        <p style="color:#3f51b5;" >Nuestro producto es 100% de calidad, siempre a la vanguardia de la moda.</p>
        <div class="button-group">
          <li  class="btn btn-default">Muchos Estilos</li>

        </div>
      </div>
    </div><!--end of row-->
  </div><!--end of container-->
</header>

<div class="action-bar">
